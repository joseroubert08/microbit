<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="@color/ok_button_down" />
            <stroke
                android:width="1dp"
                android:color="@color/ok_button_down" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/ok_button_up"/>
            <stroke
                android:width="1dp"
                android:color="@color/ok_button_up" />
        </shape>
    </item>
</selector>